{
  "name": "react-isomorphic-boilerplate",
  "version": "0.7.4",
  "main": "src/server/index.js",
  "license": "MIT",
  "engines": {
    "node": "^8.10.0 || >=9.10.0"
  },
  "scripts": {
    "start": "DEBUG=*,-nodemon*,-express*,-send,-babel*,-eslint*,-css-modules* NODE_ENV=hot npx babel-node --inspect src/server/hot/index.js",
    "build:browser:dev": "node webpack-browser-runner dev",
    "build:server:dev": "webpack --env=dev --config=webpack.server.babel.js --progress --profile --colors",
    "build:browser:prod": "node webpack-browser-runner prod",
    "build:server:prod": "webpack --env=prod --config=webpack.server.babel.js --progress --profile --colors",
    "eslint": "eslint ./",
    "test": "npm run eslint && cross-env NODE_ENV=test nyc ava --verbose",
    "coverage": "nyc report --reporter=lcov",
    "report": "cross-env NODE_ENV=test nyc ava && yarn run coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "asset-require-hook": "^1.2.0",
    "autoprefixer": "^9.5.1",
    "ava": "^1.4.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-transform-imports": "^1.5.1",
    "codecov": "^3.4.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "css-modules-require-hook": "^4.2.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^3.0.1",
    "ignore-styles": "^5.0.1",
    "mini-css-extract-plugin": "^0.6.0",
    "mkdirp": "^0.5.1",
    "mock-require": "^3.0.1",
    "node-sass": "^4.12.0",
    "nyc": "^14.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.0",
    "postcss-scss": "^2.0.0",
    "remove-debug-loader": "^0.2.2",
    "reset.css": "^2.0.2",
    "sass-loader": "^7.1.0",
    "sinon": "^7.3.2",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.31.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-hot-server-middleware": "^0.6.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "babel-plugin-react-css-modules": "^5.2.4",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.5",
    "date-fns": "^1.30.1",
    "debug": "^4.1.0",
    "express": "^4.16.2",
    "immutability-helper": "^3.0.0",
    "lodash": "^4.17.4",
    "normalizr": "^3.2.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.0.0-beta",
    "react-hot-loader": "^4.8.4",
    "react-redux": "^7.1.0-alpha.4",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "superagent": "^5.0.5"
  },
  "description": "This boilerplate would help you build a react/redux/react-router isomorphic/universal web app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ddhp/react-isomorphic-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "isomorphic",
    "universal",
    "hmr",
    "hot-reload",
    "react-router"
  ],
  "author": "ddhp",
  "bugs": {
    "url": "https://github.com/ddhp/react-isomorphic-boilerplate/issues"
  },
  "homepage": "https://github.com/ddhp/react-isomorphic-boilerplate#readme",
  "ava": {
    "files": [
      "src/**/__tests__/**/*.js",
      "src/**/*.spec.js",
      "!test/fixtures/**/*",
      "!test/helpers/**/*"
    ],
    "require": [
      "@babel/register",
      "@babel/polyfill",
      "ignore-styles",
      "css-modules-require-hook/preset",
      "./src/enzymeSetup.js"
    ]
  },
  "nyc": {
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
